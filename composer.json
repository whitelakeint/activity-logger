{
    "name": "whitelakeint/activity-logger",
    "description": "Comprehensive activity logging package for Laravel applications",
    "keywords": ["laravel", "logging", "activity", "monitoring", "tracking"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amit Shah",
            "email": "amit@whitelakedigital.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ActivityLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActivityLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ActivityLogger\\Providers\\ActivityLoggerServiceProvider"
            ],
            "aliases": {
                "ActivityLogger": "ActivityLogger\\Facades\\ActivityLogger"
            }
        }
    },
    "minimum-stability": "stable"
}